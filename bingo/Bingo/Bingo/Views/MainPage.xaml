<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Bingo.Views.MainPage" Title="{Binding Title}">
    <StackLayout BackgroundColor="White">
        <!-- Place new controls here -->
        <Label Font="30" FontAttributes="Bold" TextColor="Green" Text="Friendship Bingo" 
               HorizontalOptions="Center" VerticalOptions="CenterAndExpand" />
        <Button Text="GO" TextColor="White" BackgroundColor="Green" 
                 Margin="0, 0, 0, 150" Command="{Binding NavigateCommand}"/>
        
    </StackLayout>
</ContentPage>